{
  "name": "developergeorg/php-project-lvl1",
  "authors": [
    {
      "name": "developergeorg",
      "email": "bg@moderntoken.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "BrainGames\\": "src/"
    },
    "files": [
      "src/Cli.php",
      "src/Games/helpers.php",
      "src/Games/CliEngine.php",
      "src/Games/Calc.php",
      "src/Games/Even.php",
      "src/Games/Gcd.php",
      "src/Games/Prime.php",
      "src/Games/Progression.php"
    ]
  },
  "bin": [
    "bin/brain-games",
    "bin/brain-even",
    "bin/brain-calc",
    "bin/brain-gcd",
    "bin/brain-progression"
  ],
  "require": {
    "wp-cli/php-cli-tools": "^0.11.11",
    "ext-gmp": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*"
  },
  "scripts": {
    "phpcs": "php vendor/squizlabs/php_codesniffer/bin/phpcs"
  }
}
